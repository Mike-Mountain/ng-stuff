        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/shared-ui/src/**/*.ts",
              "libs/shared-ui/src/**/*.html"
            ]
          }
        }
      },
      "tags": []
    },
